<Window x:Class="RoadWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RoadWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel HorizontalAlignment="Stretch">
            <TextBox Width="100" x:Name="Url" Text="https://raw.githubusercontent.com/jakkaj/xIOT/master/README.md"></TextBox>
            <Button Click="ButtonBase_OnClick" Content="Get" Width="200"></Button>
            <WebBrowser x:Name="MyBrowser" HorizontalAlignment="Stretch" Height="600">

            </WebBrowser>
        </StackPanel>
        
    </Grid>
</Window>
